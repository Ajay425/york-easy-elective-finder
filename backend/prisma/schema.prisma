generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Course {
  id        Int      @id @default(autoincrement())
  dept      String 
  code      String
  credit    Float?
  name      String
  prereq 
  desc      String?
}

// MANY TO MANY
model coursePrerequisites{
  courseCode String
  prerequsites String
}

model Department{
  acronym String
  longForm String
}

model Faculty{
  acronym String
  longForm String
}

model Professor{
  id Int
  name String
}

model courseTaught{
  courseCode String
  term String
  professor String
}
